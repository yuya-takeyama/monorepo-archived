    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "glob": "7.1.6"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.35",
    "@typescript-eslint/parser": "4.18.0",
    "@vercel/ncc": "0.25.1",
    "eslint": "7.23.0",
    "eslint-plugin-github": "4.1.2",
    "eslint-plugin-jest": "24.3.3",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "js-yaml": "3.14.1",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  }
